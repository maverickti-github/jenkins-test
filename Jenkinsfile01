pipeline {
    agent any
    stages {
        stage ('Checkout'){
            steps {
                checkout smc
            }
        }
        stage ('Test') {
            steps {
                sh 'echo Hola Mundo'
                echo "Build number is ${currentBuild.number}"
                echo "Branch name is ${env.BRANCH_NAME}"
                echo "Job es ${env.JOB_NAME}"
                echo "El Workspaces is ${env.WORKSPACE}"
            }
        }
    }
}